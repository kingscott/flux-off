{
  "parser": "babel-eslint",
  "rules": {
   "indent": [ 2, 2 ],
   "quotes": [ 2, "single" ],
   "linebreak-style": [ 2, "unix" ],
   "semi": [ 2, "always" ],
   "no-console": [ 0 ],
   "no-loop-func": [ 0 ],
   "new-cap": [ 0 ],
   "no-trailing-spaces": [ 0 ]
  },
  "env": {
   "es6": true,
   "node": true,
   "browser": true
  },
  "ecmaFeatures": {
   "modules": true,
   "restParams": true,
   "spread": true
  }
}
